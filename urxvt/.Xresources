!###################
! URxvt Settings
!###################
URxvt.termName: rxvt

URxvt.font: xft:Fira Code:style=Light:size=12
URxvt.boldfont: xft:Fira Code:style=Medium:size=12
URxvt.italicfont: xft:Fira Code:style=Light:italic:size=12
URxvt.bolditalicfont: xft:Fira Code:style=Medium:italic:size=12

!URxvt*font: xft:Fira Code:style=Light:size=12

URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.internalBorder: 10
URxvt.cursorBlink: true
URxvt.saveLine: 2048
URxvt.urgentOnBell: true
URxvt.depth: 32

! Underlined and clickable URLs
URxvt.urlLauncher: firefox 
URxvt.matcher.button: 1

! Resize font size
URxvt.resize-font.smaller: C-minus
URxvt.resize-font.bigger: C-plus
URxvt.resize-font.reset: C-equal
URxvt.resize-font.show: C-question

URxvt.iso14755: false
URxvt.iso14755_52: false
